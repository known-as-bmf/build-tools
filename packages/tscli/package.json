{
  "name": "@known-as-bmf/tscli",
  "version": "0.1.6",
  "description": "Typescript build & test CLI",
  "author": "Julien Avert",
  "license": "MIT",
  "bin": {
    "tscli": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node ./bin/run build",
    "lint": "node ./bin/run lint",
    "readme": "oclif-dev readme",
    "postpublish": "rimraf oclif.manifest.json",
    "prepublishOnly": "node ./bin/run build && oclif-dev manifest"
  },
  "dependencies": {
    "@babel/core": "7.12.3",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.3",
    "@oclif/parser": "3.8.5",
    "@oclif/plugin-help": "3.2.0",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@rollup/plugin-typescript": "6.1.0",
    "babel-jest": "26.6.1",
    "brotli-size": "4.0.0",
    "builtin-modules": "3.1.0",
    "eslint": "7.12.1",
    "find-up": "5.0.0",
    "fs-extra": "9.0.1",
    "globby": "11.0.1",
    "gzip-size": "5.1.1",
    "jest": "26.6.1",
    "jest-config": "26.6.1",
    "pretty-bytes": "5.4.1",
    "progress-estimator": "0.3.0",
    "rollup": "2.33.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "superstruct": "0.10.12",
    "ts-jest": "26.4.3",
    "tslib": "2.0.3",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@jest/types": "26.6.1",
    "@known-as-bmf/eslint-config-bmf": "workspace:*",
    "@oclif/dev-cli": "1.22.2",
    "@types/eslint": "7.2.4",
    "@types/fs-extra": "9.0.2",
    "@types/node": "14.14.6",
    "@types/validate-npm-package-name": "3.0.0",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "tscli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
